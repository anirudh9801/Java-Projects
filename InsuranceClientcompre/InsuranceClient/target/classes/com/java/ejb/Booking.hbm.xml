<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.java.ejb.Booking" table="booking">
        <id name="PolicyId" type="int">
            <column name="PolicyId" />
            <generator class="identity" />
        </id>
        <property name="custId" type="int">
            <column name="custId" />
        </property>
        <property name="planId" type="int">
            <column name="PlanId" />
        </property>
        <property name="insuranceId" type="string">
            <column name="insuranceId" />
        </property>
        <property name="PremiumAmount" type="string">
            <column name="PremiumAmount" />
        </property>
        <property name="Paymode" type="string">
            <column name="Paymode" />
        </property>
        <property name="InstallmentAmount" type="double">
            <column name="InstallmentAmount" />
        </property>
        <property name="RegisterDate" type="date">
            <column name="RegisterDate" />
        </property>
    </class>
</hibernate-mapping>
