package com.java.Prog;

public class DemoClass {

    public static void sampleMethod(int a, int b) throws ArithmeticException {
        System.out.println("Hello, this is sample method");
        int c = a / b;
        System.out.println("c: " + c);
    }

    // Overloaded method to handle double parameters
    public static void sampleMethod(double a, double b) throws ArithmeticException {
        System.out.println("Hello, this is overloaded sample method");
        double c = a / b;
        System.out.println("c: " + c);
    }

    public static void main(String[] args) {
        sampleMethod(4, 2);           // Calls the int version
        sampleMethod(4.0, 2.0);       // Calls the double version
    }
}
