<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee /WEB-INF/schema/web-facesconfig_2_0.xsd"
	version="2.0" metadata-complete="false">
	<!-- This file must be present with our jar, even if it is empty. Otherwise, 
		our Java annotations won't get scanned! -->
	<managed-bean>
		<managed-bean-name>doctors</managed-bean-name>
		<managed-bean-class>com.java.hms.Doctors</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>doctorsDao</managed-bean-name>
		<managed-bean-class>com.java.hms.DoctorsDaoImpl</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>roomDao</managed-bean-name>
		<managed-bean-class>com.java.hms.RoomDaoImpl</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>patientDao</managed-bean-name>
		<managed-bean-class>com.java.hms.PatientDaoImpl</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>patient</managed-bean-name>
		<managed-bean-class>com.java.hms.Patient</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>


</faces-config>