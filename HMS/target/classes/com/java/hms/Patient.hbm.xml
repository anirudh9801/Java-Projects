<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class dynamic-insert="false" dynamic-update="false" mutable="true" name="com.java.hms.Patient" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="patient_master">
        <id name="pid" type="java.lang.String">
            <column name="pid"/>
            <generator class="assigned"/>
        </id>
        <property generated="never" lazy="false" name="name" optimistic-lock="true" type="java.lang.String" unique="false">
            <column name="name"/>
        </property>
        <property generated="never" lazy="false" name="age" optimistic-lock="true" type="java.lang.Integer" unique="false">
            <column name="age"/>
        </property>
        <property generated="never" lazy="false" name="weight" optimistic-lock="true" type="java.lang.Integer" unique="false">
            <column name="weight"/>
        </property>
        <property generated="never" lazy="false" name="gender" optimistic-lock="true" type="java.lang.String" unique="false">
            <column name="gender"/>
        </property>
        <property generated="never" lazy="false" name="address" optimistic-lock="true" type="java.lang.String" unique="false">
            <column name="address"/>
        </property>
        <property generated="never" lazy="false" name="phoneno" optimistic-lock="true" type="java.lang.String" unique="false">
            <column name="phoneno"/>
        </property>
        <property generated="never" lazy="false" name="disease" optimistic-lock="true" type="java.lang.String" unique="false">
            <column name="disease"/>
        </property>
        <property generated="never" lazy="false" name="doctorid" optimistic-lock="true" type="java.lang.String" unique="false">
            <column name="doctor_id"/>
        </property>
    </class>
</hibernate-mapping>
