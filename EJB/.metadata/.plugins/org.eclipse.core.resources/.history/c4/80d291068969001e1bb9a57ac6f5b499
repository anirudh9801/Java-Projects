public class Main {
	public static void main(String[] args) {
		
		try {
			CalculationBeanRemote service = 
					(CalculationBeanRemote) new InitialContext().lookup("CalculationBean/remote");
			
		} catch (NamingException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}


}
